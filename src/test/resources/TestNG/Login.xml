<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Havana Login Suite" thread-count="1" parallel="methods">
	<listeners>
		<listener class-name="utils.listeners.TestListener" />
		<listener class-name="utils.listeners.AnnotationTransformer" />
	</listeners>
	<test name="Login Suite">
		<!--	chrome/edge/firefox	-->
		<parameter name="browser" value="chrome" />
		<parameter name="mode" value="UI" />
		<parameter name="environment" value="sandbox" />
		<parameter name="client" value="havana" />
		<parameter name="implicitwaitduration" value="10" />
		<parameter name="timeunit" value="seconds" />
		<!--	windows/mac/linux	-->
		<parameter name="os" value="windows" />
		<!-- <parameter name="os" value="${testos}" /> -->
		<parameter name="url" value="" />
		<!-- <parameter name="plantype" value="All" /> -->

	
		<classes>
			<class name="tests.Havana_TestCases.HavanaRecruiter">

			</class>
			<class name="tests.Havana_TestCases.HavanaRecruitingManager">

			</class>
			<class name="tests.Havana_TestCases.HavanaHRManager">

			</class>
		</classes>
	</test>
</suite>